#include <stdio.h>
int main()
{
    int n,i,factor=0;
    printf("enter the number:");
    scanf("%d",&n);
    for(i=2;i<n;i++)
    {
        if(i % n ==0)
        {
            factor=factor+1;
        }
    }
    if(factor==0)
    {
        printf("%d is a prime number");
    } 
    else
    {
        printf("%d is not a prime number");
    }
    return 0;
}

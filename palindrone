#include <stdio.h>

int main()
{
    int n,reversed=0,remainder,original;
    printf("enter the number:");
    scanf("%d",&n);
    original = n;
    while (n != 0)
    {
        remainder = n % 10;
        reversed = reversed * 10 + remainder;
        n /= 10;
    }
    if(original==reversed)
    {
        printf("is a palindrome:%d",original);
    }
    else
    {
        printf("is not apalindrome:%d",original);
    }
    
	
	return 0;
}

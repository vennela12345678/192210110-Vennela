#include <stdio.h>

int main()
{
	int n,m,i,gcd;
	printf("enter the first number:");
	scanf("%d",&n);
	printf("enter the second number: ");
	scanf("%d",&m);
	for(i=1;i<=n && i<=m;i++)
	{
	    if(n%i ==0 && m%i ==0)
	    {
	        gcd=i;
	    }
	}
	printf("gcd of %d and %d is %d",n,m,gcd);
	    
	
	return 0;
}
